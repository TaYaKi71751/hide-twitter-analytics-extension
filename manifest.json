{
  "manifest_version": 2,
  "name": "Twitter Rollback Analytics",
  "version": "1.0",
  "description": "Hide some analytics elements in some Tweet Views",
  "icons": {
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "js": ["hide.js"],
						"run_at": "document_end"
    }
  ]
}

